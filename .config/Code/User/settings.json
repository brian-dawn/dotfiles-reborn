{
    "vim.disableAnnoyingNeovimMessage": true,
    "workbench.colorTheme": "Arc+ (Light)",
    "editor.fontFamily": "Source Code Pro",
    "clang.cxxflags": [
        "-std=c++14"
    ],
    "C_Cpp.intelliSenseEngine": "Default",
    "vim.enableNeovim": true,
    "vim.neovimPath": "/usr/bin/nvim",
    "vim.easymotion": true,
    "vim.leader": "<space>",
    "vim.easymotionMarkerFontFamily": "Source Code Pro",
    "vim.useSystemClipboard": true,
    "rust.mode": "rls",
    "elm.formatOnSave": true,
    "rust.rustup": {
        "toolchain": "nightly-x86_64-unknown-linux-gnu",
        "nightlyToolchain": "nightly-x86_64-unknown-linux-gnu"
    },
    "rust.rls": {
        "useRustfmt": true
    },
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    "breadcrumbs.enabled": true,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "none",
    "editor.renderIndentGuides": false,
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "hidden",
    "rust-analyzer.enableEnhancedTyping": false,
    "window.menuBarVisibility": "toggle",
    "python.jediEnabled": false,
    "window.menuBarVisibility": "toggle",
    "files.autoSave": "afterDelay",
    "terminal.integrated.inheritEnv": false,
    "python.pythonPath": "/usr/bin/python",
    "python.formatting.provider": "black",
    "rust-analyzer.displayInlayHints": false,
    "python.languageServer": "Microsoft",
}
